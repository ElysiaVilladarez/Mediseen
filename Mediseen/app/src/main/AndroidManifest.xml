<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mediseen.work.pearlsantos.mediseen" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:windowSoftInputMode="adjustPan"
            android:parentActivityName="mediseen.login.SplashScreen">
        </activity>
        <activity
            android:name="mediseen.login.CreateAccount"
            android:label="@string/app_name"
            android:windowSoftInputMode="adjustPan"
            android:parentActivityName="mediseen.login.SplashScreen">
        </activity>
        <activity
            android:name="mediseen.login.SetAccount"
            android:label="@string/app_name"
            android:windowSoftInputMode="adjustPan"
            android:parentActivityName="mediseen.login.SplashScreen">
        </activity>

        <activity
            android:name="mediseen.login.SplashScreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name="mediseen.login.LoginPage"
            android:label="@string/app_name"
            android:parentActivityName="mediseen.login.SplashScreen">>
        </activity>
        <!--<activity-->
            <!--android:name="mediseen.helpers.TestLayout"-->
            <!--android:label="@string/title_activity_test_layout"-->
            <!--android:theme="@style/AppTheme"-->
            <!--android:windowSoftInputMode="adjustPan">-->
        <!--</activity>-->
    </application>

</manifest>
